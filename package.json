{
  "name": "botdiscord",
  "version": "0.1.0",
  "description": "Bot Discord.js",
  "main": "main.js",
  "repository": "https://github.com/Thoms38/botdiscordjsts.git",
  "author": "Thoms38 <thomas.saladini@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "discord.js": "discordjs/discord.js"
  },
  "devDependencies": {
    "eslint": "^6.3.0",
    "eslint-config-aqua": "^6.0.2"
  },
  "eslintConfig": {
    "extends": "aqua/node",
    "rules": {
      "linebreak-style": [
        "error",
        "windows"
      ],
      "quotes": [
        "warn",
        "double"
      ],
      "indent": [
        "error",
        2
      ],
      "curly": "off"
    }
  }
}
